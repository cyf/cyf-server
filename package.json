{
  "name": "cyf-server",
  "version": "0.0.1-alpha.0",
  "private": true,
  "description": "Api Server for cyf.",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run gen:css && prisma generate && prisma db push --accept-data-loss && nest build",
    "dev": "prisma generate && nest start",
    "dev:debug": "prisma generate && nest start --debug --watch",
    "dev:watch": "prisma generate && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "format": "prettier --write \"**/*.ts\"",
    "gen:css": "postcss main.css -o ./public/style.css",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky",
    "start": "cross-env NODE_ENV=production node dist/main",
    "test": "jest --detectOpenHandles --forceExit",
    "test:cov": "jest --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/common/(.*)$": "<rootDir>/common/$1",
      "^@/modules/(.*)$": "<rootDir>/modules/$1"
    },
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest"
      ]
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.620.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/websockets": "^10.3.10",
    "@prisma/client": "^5.17.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.7.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compare-versions": "^6.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.12",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "nestjs-form-data": "^1.9.91",
    "nestjs-i18n": "^10.4.5",
    "nestjs-throttler-storage-redis": "^0.4.4",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pug": "^3.0.3",
    "query-string": "^7.1.3",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "sort-keys-recursive": "^2.1.10",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.13",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.7",
    "@types/mime": "^3.0.4",
    "@types/node": "^18.19.42",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/webpack-env": "^1.18.5",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "run-script-webpack-plugin": "^0.2.0",
    "sort-package-json": "^2.10.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.7",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.4.5",
    "webpack": "^5.93.0",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": "^18"
  }
}
